<odoo>
    <data>

        <template id="cpo_new_list_link">
            <ul>
                <t t-foreach="objects" t-as="object">
                    <li>
                        <a t-attf-href="/news/#{ object.id }">
                            <span><t t-esc="object.cpo_new_date"/></span>
                            <t t-esc="object.cpo_new_title"/>
                        </a>
                    </li>
                </t>
