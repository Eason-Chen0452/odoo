<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!--BOM管理员-->
    <record id="cpo_operation_authority" model="res.groups">
        <field name="name">Web Page Operation authority</field>
        <field name="users" eval="[(4, ref('base.user_root'))]"/>
    </record>

    <record id="cpo_website_aperation_authority" model="res.groups">
        <field name="name">Front-end function settings</field>
        <field name="users" eval="[(4, ref('base.user_root'))]"/>
    </record>

    <record id="cpo_quotation_record_aperation_authority" model="res.groups">
        <field name="name">Quotation record viewing permission</field>
        <field name="users" eval="[(4, ref('base.user_root'))]"/>
    </record>

    <record id="cpo_quotation_record_super_aperation_authority" model="res.groups">
        <field name="name">Quotation record super permission</field>
        <field name="users" eval="[(4, ref('base.user_root'))]"/>
    </record>



</odoo>