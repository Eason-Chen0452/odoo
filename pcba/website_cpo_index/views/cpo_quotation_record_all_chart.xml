<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>

		<!--	PCB quotation record chart（图表）Begin	-->
        <record id="cpo_pcb_record_graph" model="ir.ui.view">
            <field name="name">PCB Quotation Graph</field>
            <field name="model">cpo_pcb_record</field>
            <field name="arch" type="xml">
                <graph string="PCB" >
                     <field name="user_name" />
                     <field name="cpo_country" />
                </graph>
            </field>
        </record>
        <record model="ir.ui.view" id="cpo_pcb_record_calendar_view">
            <field name="name">PCB Quotation Calendar</field>
            <field name="model">cpo_pcb_record</field>
            <field name="arch" type="xml">
                <calendar string="Ideas" date_start="cpo_time" >
                    <field name="cpo_country"/>
                    <field name="user_name"/>
                </calendar>
            </field>
        </record>
		<!--	PCB quotation record chart（图表）End	-->

