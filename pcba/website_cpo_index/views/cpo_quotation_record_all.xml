        <!-- PCB 报价记录 Tree -->
        <record model="ir.ui.view" id="cpo_pcb_record_all_pcb_tree">
            <field name="name">PCB Quotation Tree</field>
            <field name="model">cpo_pcb_record</field>
            <field name="arch" type="xml">
                <tree>
                    <field name="sequence" widget="handle"/>
                    <!--<field name="order_type"/>-->
                    <field name="quantity"/>
                    <field name="width"/>
                    <field name="lenght"/>
                    <field name="panel_size"/>
                    <field name="material"/>
                    <field name="inner_copper"/>
                    <field name="outer_copper"/>
                    <field name="layers"/>
                    <field name="mask_color"/>
                    <field name="silkscreen_color"/>
