                    <field name="e_test"/>
                    <field name="thickne"/>
                    <field name="cpo_country"/>
                    <!--<field name="cpo_city"/>-->
                    <!--<field name="user_ip"/>-->
                    <!--<field name="partner_info"/>-->
                    <field name="total"/>
                    <field name="cpo_time"/>
                    <field name="cpo_code"/>
                    <field name="user_info"/>
                    <field name="cpo_intention"/>
					<field name="user_name" invisible="1"/>
                </tree>
            </field>
        </record>

        <!-- PCBA 报价记录 Tree -->
        <record model="ir.ui.view" id="cpo_pcb_record_all_pcba_tree">
