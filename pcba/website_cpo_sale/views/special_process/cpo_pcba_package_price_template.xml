    <!-- 右边布局 Version-0.3 不含物料打包价 -->
    <template id="cpo_pcba_package_calculation_results">
        <div class="pcba_nomaterial_package_calculation_results">
            <t t-call="website_cpo_sale.cpo_pcba_upload_file_new"/>
            <t t-call="website_cpo_sale.upload_file_05"/>
            <div class="cpo_smt_assembly_cost">
                <button class="btn btn-primary cpo_calculation"><i class="fa fa-calculator fa-lg"></i>Calculation</button>
            </div>
            <p class="cpo_price_reminder">Click the button to quote !</p>
            <div class="calculation_content cpo-none">
                <form class="form-horizontal"
                            t-attf-action="/shop/product/#{ slug(product_id) }"
                            method="post"
                            action="/get_pcba_detail"
                            role="form"
                            id="cpo_pcba_package_no_material">
                            <input type="hidden" name="csrf_token" t-att-value="request.csrf_token()" />
                    <div class="cpo_cost_fee">
                        <!--Quantity-->
                        <div class="cpo_assembly_cost cpo_quantity_qty">
                            <span class="cpo_cost_span">Production Quantity</span>
                            <span class="cpo_cost_fee cpo-pdl-0">
                                <input type="text" class="cpo_pcba_quantity" name="cpo_pcba_quantity" value="0"  readonly="readonly"/>
                            </span>
                        </div>
                        <div class="cpo_assembly_cost">
                            <span class="cpo_cost_span">Total</span>
                            <span class="cpo_cost_fee">
                                <i>$</i>
                                <input type="text" class="total_cost" name="total_cost" value="0.00"  readonly="readonly"/>
                            </span>
                        </div>
                        <!--多种数量比较-->
                        <div class="cpo_try_order_quantity pcba_try_quantity cpo-none">
                            <p class="cpo_try_quantity_title">
                                <!--<span>Choose</span>-->
                                <span>Quantity</span>
                                <span>Total</span>
                            </p>
                            <div class="cpo_quantity_content">
                                <div class="cpo_quantiry_line cpo_quantity_select">
                                    <div class="cpo_try_select_icon"></div>
                                    <p class="cpo_try_quantity">
                                        <span>100</span>
                                        <span>$ 120</span>
                                    </p>
                                </div>
                                <div class="cpo_quantiry_line">
                                    <div class="cpo_try_icon"></div>
                                    <p class="cpo_try_quantity">
                                        <span>150</span>
                                        <span>$ 170</span>
                                    </p>
                                </div>
                                <div class="cpo_quantiry_line">
                                    <div class="cpo_try_icon"></div>
                                    <p class="cpo_try_quantity">
                                        <span>200</span>
                                        <span>$ 210</span>
                                    </p>
                                </div>
                            </div>
                        </div>
                        <div class="cpo_step_next">
                            <button type="submit" class="btn btn-primary" id="step_next" data-toggle="modal" data-target="#cpo_need_pcb_modal">Next step</button>
                            <!--<i class="fa fa-calculator fa-lg"></i>-->

                            <div id="cpo_pcba_result"></div>
                            <div class="file_ids cpo-none">
                                <div class="cpo_file_ids">
                                    <t t-if="edit or login">
                                        <t t-if="edit_values.get('ir_file_ids')">
                                            <t t-foreach="edit_values.get('ir_file_ids')" t-as="ir_f">
                                                <t t-if="ir_f.description == 'Gerber File'">
                                                    <input type="hidden" name="gerber_atta_id" t-att-value="ir_f.id"/>
                                                    <input type="hidden" name="gerber_file_id" t-att-value="ir_f.res_id"/>
                                                    <input type="hidden" name="gerber_file_name" t-att-value="ir_f.name"/>
                                                </t>
                                                <t t-if="ir_f.description == 'BOM File'">
                                                    <input type="hidden" name="bom_atta_id" t-att-value="ir_f.id"/>
                                                    <input type="hidden" name="bom_file_id" t-att-value="ir_f.res_id"/>
                                                    <input type="hidden" name="bom_file_name" t-att-value="ir_f.name"/>
                                                </t>
                                                <t t-if="ir_f.description == 'SMT File'">
                                                    <input type="hidden" name="smt_atta_id" t-att-value="ir_f.id"/>
                                                    <input type="hidden" name="smt_file_id" t-att-value="ir_f.res_id"/>
                                                    <input type="hidden" name="smt_file_name" t-att-value="ir_f.name"/>
                                                </t>
                                            </t>
                                        </t>
                                    </t>
                                </div>
                                <div class="gerber_box">
                                    <input name="gerber_file_id" value="" class="gerber_file_id"/>
                                    <input name="gerber_atta_id" value="" class="gerber_atta_id"/>
                                    <input name="gerber_file_name" value="" class="gerber_file_name"/>
                                </div>
                                <div class="bom_box">
                                    <input name="bom_file_id" value="" class="bom_file_id"/>
                                    <input name="bom_atta_id" value="" class="bom_atta_id"/>
                                    <input name="bom_file_name" value="" class="bom_file_name"/>
                                </div>
                                <div class="smt_box">
                                    <input name="smt_file_id" value="" class="smt_file_id"/>
                                    <input name="smt_atta_id" value="" class="smt_atta_id"/>
                                    <input name="smt_file_name" value="" class="smt_file_name"/>
                                </div>
                            </div>
                            <div class="all-package-quantity cpo-none">
                                <input type="hidden" class="cpo_quantity" name="cpo_quantity" value=""/>
                            </div>
                            <div id="cpo_pcba_result" class="cpo-none">

                            </div>
                            <div class="pcb_edit cpo-none">
                                <t t-if="edit">
                                    <input type="hidden" name="edit_id" t-att-value="edit_values.get('edit_id')"/>
                                </t>
                            </div>
                        </div>
                    </div>
                </form>
            </div>
            <div class="order_page_ad">
                <div class="page-close-btn"><i class="fa fa-times-circle fa-2x"></i></div>
                <a class="order-page-content"></a>
            </div>
        </div>
    </template>

