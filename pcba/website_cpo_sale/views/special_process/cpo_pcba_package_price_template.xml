<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="cpo_pages_assets_frontend" inherit_id="website.assets_frontend" name="Website Pages">
        <xpath expr="." position="inside">
        </xpath>
        <xpath expr="//script[last()]" position="after">
            <script type="text/javascript" src="/website_cpo_sale/static/src/js/pcb_package_price/cpo_pcb_package_price.js"></script>
        </xpath>
    </template>
    <!-- 不需要可供物料 -->
    <template id="website_smt_no_material_template" name="PCBA Package Price">
         <t t-call="website.layout">
             <t t-set="additional_title">Package Price</t>
             <!-- 登录页面 -->
             <t t-call="website_cpo_sale.cpo_pcb_login_signup"/>
             <t t-call="website_cpo_sale.pcba_package_price_quotation_online"/>
         </t>
    </template>
    <!-- 需要可供物料 -->
    <template id="website_smt_material_template" name="PCBA Package Price">
         <t t-call="website.layout">
             <t t-set="additional_title">Package Price</t>
             <!-- 登录页面 -->
             <t t-call="website_cpo_sale.cpo_pcb_login_signup"/>
             <t t-call="website_cpo_sale.pcba_package_price_material"/>
         </t>
    </template>

     <!-- //////-含物料-////////////////////////////////////////////////////////////////////////////////////////////////////////////////// -->

    <!--PCBA 数据 Version-0.3 不含物料打包价  -->
    <template id="pcba_package_price_quotation_online">
        <div class="row" id="xiangdao">
            <div class="col-md-1"></div>
            <div class="col-md-10">
                <t t-call="website_sale.wizard_checkout">
                    <t t-set="step" t-value="5"/>
                </t>
            </div>
            <div class="col-md-1"></div>
        </div>
        <div class="container">
            <!-- 未注册/登录先让用户注册/登录 -->
            <t t-if="cpo_login">
                <t t-call="website_cpo_sale.cpo_register_new_user"/>
            </t>
            <div class="row">
                <!--<div id="xiangdao">-->
                    <!--<t t-call="website_sale.wizard_checkout">-->
                        <!--<t t-set="step" t-value="60" />-->
                    <!--</t>-->
                <!--</div>-->
                <div class="pcba_pack_nav">
                    <div class="order-nav-list">
                        <ul class="order-nav-ul">
                            <li class="order-nav-li order-nav-active">
                                <a href="/pcba?type=nomaterial">Order SMT Get PCB Free</a>
                            </li>
                            <li class="order-nav-li">
                                <a href="/pcba?type=material">Order SMT+Components Get PCB Free</a>
                            </li>
                            <li class="order-nav-li">
                                <a href="/pcba?type=standard">SMT Process Get Quote</a>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
            <div class="row cpo_content_row">
                <!--<t t-call="website_cpo_sale.cpo_pcba_upload_file"/>-->
                <table class="table table-bordered cpo-pcba-pack-tips">
                    <caption id="tab_title">Online offer <span>(Components Provided by Customer; Order Quantity ≤ 20 PCS; Parts Quantity ≤ 50, Get PCB Free.)</span></caption>
                </table>
                <!--错误提示框-->
                <div class="cpo-warning"></div>
                <div class="col-md-8 cpo_col_pcba_md8">
                    <div id="cpo_pcba_online_content">
                        <t t-call="website_cpo_sale.cpo_source_template"/>
                        <t t-if="edit or login">
                            <t t-call="website_cpo_sale.cpo_pcba_package_quotation_form"/>
                        </t>
                        <t t-if="not edit and not login">
                            <t t-call="website_cpo_sale.cpo_pcba_package_quotation_form_edit_05"/>
                        </t>
                    </div>
                    <!--<div id="cpo_pcb_online_content">-->
                        <!--<div class="cpo_pcb_requirements">-->
                            <!--<t t-call="website_cpo_sale.cpo_pcb_online_quotation"/>-->
                        <!--</div>-->
                    <!--</div>-->
                </div>
                <div class="col-md-4 p-0">
                    <div id="cpo_calcula_title">
                        <t t-call="website_cpo_sale.cpo_pcba_package_calculation_results"/>
                    </div>
                </div>

            </div>
        </div>
    </template>
    <!-- PCBA 数据 不含物料打包价-->
    <template id="cpo_pcba_package_quotation_form">
        <form class="form-horizontal" role="form" method="POST" action="/get_pcba_price" enctype="multipart/form-data"
            id="pcba_package_data_quotation">
            <!--数量 长宽-->
            <div class="form-group">
                <div class="col-md-6 col-sm-6">
                    <div class="cpo-box">
                        <div class="cpo-box-title">Production Quantity</div>
                        <div class="cpo-box-content">
                            <input type="text" class="cpo-box-input" name="cpo_input_quantity"
                                   t-att-value="int(edit_values.get('product_uom_qty'))" placeholder="Quantity"
                                data-toggle="tooltip" data-placement="top" title="Order Quantity ≤ 20 PCS"
                            onkeyup="if(this.value.length==1){this.value=this.value.replace(/[^1-9]/g,'')}else{this.value=this.value.replace(/\D/g,'')}"/>
                            <span class="cpo-ps-rl cpo-box-unit">
                                <i class="cpo-box-radio cpo-radio-select"></i>
                                <span>PCS</span>
                            </span>
                        </div>
                    </div>
                </div>
                <div class="col-md-6 col-sm-6">
                    <div class="cpo-box">
                        <div class="cpo-box-title">PCB/PCB panel size</div>
                        <div class="cpo-box-content">
                            <input type="text" class="cpo-box-input cpo-box-length" name="cpo_input_lenght"
                                   t-att-value="edit_values.get('pcb_length')" placeholder="Length"
                                data-toggle="tooltip" data-placement="top" title="Length ≤ 100 ㎜" />
                            <span class="cpo-box-unit">×</span>
                            <input type="text" class="cpo-box-input cpo-box-width" name="cpo_input_width"
                                   t-att-value="edit_values.get('pcb_width')" placeholder="Width"
                                data-toggle="tooltip" data-placement="top" title="Width ≤ 100 ㎜" />
                            <span class="cpo-box-unit">MM</span>
                        </div>
                    </div>
                </div>
            </div>
            <!--DIP SMT-->
            <div class="form-group">
                <div class="col-md-6 col-sm-6">
                    <div class="cpo-box">
                        <div class="cpo-box-title">DIP through hole quantity</div>
                        <div class="cpo-box-content">
                            <input type="text" class="cpo-box-input" name="cpo_input_dip" t-att-value="edit_values.get('smt_plug_qty')"
                                   placeholder="DIP through hole quantity" data-toggle="tooltip" data-placement="top" title="DIP through hole quantity ≤ 50" />
                        </div>
                    </div>
                </div>
                <div class="col-md-6 col-sm-6">
                    <div class="cpo-box">
                        <div class="cpo-box-title">SMT parts quantity</div>
                        <div class="cpo-box-content">
                            <input type="text" class="cpo-box-input" name="cpo_input_smt"
                                    t-att-value="edit_values.get('smt_component_qty')" placeholder="SMT parts quantity"
                                data-toggle="tooltip" data-placement="top" title="Parts Quantity ≤ 50" />
                        </div>
                    </div>
                </div>
            </div>
            <!--板厚 单双面贴片-->
            <div class="form-group">
                <div class="col-md-6 col-sm-6">
                    <div class="cpo-box">
                        <div class="cpo-box-title">Single side/double side SMT</div>
                        <div class="cpo-box-content">
                            <t t-set="layer_pcb" t-value="edit_values.get('layer_pcb')"/>
                            <select class="cpo-box-select pcba_smt_side" disabled="disabled">
                                <option value="1">Single side</option>
                                <option value="2">Double side</option>
                            </select>
                        </div>
                    </div>
                </div>
                <div class="col-md-6 col-sm-6">
                    <div class="cpo-box">
                        <div class="cpo-box-title">Board thickness</div>
                        <div class="cpo-box-content">
                            <input type="text" class="cpo-box-input" disabled="disabled" name="cpo_input_thick"
                                   t-att-value="edit_values.get('pcb_thickness')"  placeholder=""/>
                            <span class="cpo-box-unit">MM</span>
                        </div>
                    </div>
                </div>
            </div>
            <!--其他需求-->
            <div class="form-group">
                <div class="col-md-6 col-sm-6">
                    <div class="cpo-box">
                        <div class="cpo-box-title">Surface Treatment</div>
                        <div class="cpo-box-content">
                            <select class="cpo-box-select surface_value" disabled="disabled">
                                <option value="Lead Free HASL">Lead Free HASL</option>
                                <!--<option value="No">No</option>-->
                            </select>
                        </div>
                    </div>
                </div>
                <!--<div class="col-md-6 col-sm-6">-->
                    <!--<div class="cpo-box cpo-none">-->
                        <!--<div class="cpo-box-title">Components supply</div>-->
                        <!--<div class="cpo-box-content">-->
                            <!--<select class="cpo-box-select pcba_package_included">-->
                                <!--<option value="no">Not included</option>-->
                                <!--<option value="yes">Included</option>-->
                            <!--</select>-->
                        <!--</div>-->
                    <!--</div>-->
                <!--</div>-->
            </div>
        </form>
    </template>
