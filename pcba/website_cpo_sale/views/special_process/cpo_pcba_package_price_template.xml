        <form class="form-horizontal" role="form" method="POST" action="/get_pcba_price" enctype="multipart/form-data"
            id="pcba_package_data_quotation">
            <!--数量 长宽-->
            <div class="form-group">
                <div class="col-md-6 col-sm-6">
                    <div class="cpo-box">
                        <div class="cpo-box-title">Production Quantity</div>
                        <div class="cpo-box-content">
                            <input type="text" class="cpo-box-input" name="cpo_input_quantity" value="5" placeholder="Quantity"
                                data-toggle="tooltip" data-placement="top" title="Order Quantity ≤ 5 PCS" />
                            <span class="cpo-ps-rl cpo-box-unit">
                                <i class="cpo-box-radio cpo-radio-select"></i>
                                <span>PCS</span>
                            </span>
                        </div>
                    </div>
                </div>
                <div class="col-md-6 col-sm-6">
                    <div class="cpo-box">
                        <div class="cpo-box-title">PCB/PCB panel size</div>
                        <div class="cpo-box-content">
                            <input type="text" class="cpo-box-input cpo-box-length" name="cpo_input_lenght" value="100" placeholder="Length"
                                data-toggle="tooltip" data-placement="top" title="Length ≤ 100 ㎜" />
                            <span class="cpo-box-unit">×</span>
                            <input type="text" class="cpo-box-input cpo-box-width" name="cpo_input_width" value="100" placeholder="Width"
                                data-toggle="tooltip" data-placement="top" title="Width ≤ 100 ㎜" />
                            <span class="cpo-box-unit">MM</span>
                        </div>
                    </div>
                </div>
            </div>
            <!--DIP SMT-->
            <div class="form-group">
                <div class="col-md-6 col-sm-6">
                    <div class="cpo-box">
                        <div class="cpo-box-title">Parts Types</div>
                        <div class="cpo-box-content">
                            <input type="text" class="cpo-box-input" name="cpo_material_type" value="25"
                                   placeholder="Material type quantity" data-toggle="tooltip" data-placement="top" title="Parts Types ≤ 50"/>
                        </div>
                    </div>
                </div>
                <div class="col-md-6 col-sm-6">
                    <div class="cpo-box">
                        <div class="cpo-box-title">Parts Quantity</div>
                        <div class="cpo-box-content">
                            <input type="text" class="cpo-box-input" name="cpo_input_smt" value="25" placeholder="SMT parts quantity"
                                data-toggle="tooltip" data-placement="top" title="Parts Quantity ≤ 50" />
                        </div>
                    </div>
                </div>
            </div>
            <!--板厚 单双面贴片-->
            <div class="form-group">
                <div class="col-md-6 col-sm-6">
                    <div class="cpo-box">
                        <div class="cpo-box-title">Single side/double side SMT</div>
                        <div class="cpo-box-content">
                            <select class="cpo-box-select pcba_smt_side" disabled="disabled">
                                <option value="1">Single side</option>
                                <option value="2">Double side</option>
                            </select>
                        </div>
                    </div>
                </div>
                <div class="col-md-6 col-sm-6">
                    <div class="cpo-box">
                        <div class="cpo-box-title">Board thickness</div>
                        <div class="cpo-box-content">
                            <input type="text" class="cpo-box-input" disabled="disabled" name="cpo_input_thick" value="1.6" placeholder=""/>
                            <span class="cpo-box-unit">MM</span>
                        </div>
                    </div>
                </div>
            </div>
            <!--其他需求-->
            <div class="form-group">
                <div class="col-md-6 col-sm-6">
                    <div class="cpo-box">
                        <div class="cpo-box-title">Surface Treatment</div>
                        <div class="cpo-box-content">
                            <select class="cpo-box-select surface_value" disabled="disabled">
                                <option value="Lead Free HASL">Lead Free HASL</option>
                            </select>
                        </div>
                    </div>
                </div>
                <!--<div class="col-md-6 col-sm-6">-->
                    <!--<div class="cpo-box cpo-none">-->
                        <!--<div class="cpo-box-title">Components supply</div>-->
                        <!--<div class="cpo-box-content">-->
                            <!--<select class="cpo-box-select pcba_package_included">-->
                                <!--<option value="no">Not included</option>-->
                                <!--<option value="yes">Included</option>-->
                            <!--</select>-->
                        <!--</div>-->
                    <!--</div>-->
                <!--</div>-->
            </div>
        </form>
    </template>
    <!-- PCBA含物料打包价编辑 0.5 开始 -->
    <template id="cpo_pcba_package_material_quotation_edit_05">
